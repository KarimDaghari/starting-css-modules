{
  "name": "starting-css-modules",
  "version": "1.0.0",
  "description": "A very simple example of css-modules",
  "scripts": {
    "clean": "rm -fR dist",
    "copy": "cp -R src dist",
    "prebuild": "npm run clean && npm run copy",
    "build": "monobrow -c monobrow.config.js",
    "start": "npm run build && ecstatic dist",
    "lint": "standard"
  },
  "author": "joshwnj",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babelify": "^6.1.3",
    "css-modulesify": "git://github.com/css-modules/css-modulesify#postcss-after",
    "ecstatic": "^0.8.0",
    "monobrow": "^1.1.2",
    "postcss-australian-stylesheets": "0.0.6",
    "postcss-css-variables": "^0.4.0",
    "postcss-custom-media": "^4.1.0",
    "postcss-import": "^6.1.1",
    "postcss-nested": "^0.3.2",
    "standard": "^4.5.4"
  }
}
