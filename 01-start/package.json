{
  "name": "starting-css-modules",
  "version": "1.0.0",
  "description": "A very simple example of css-modules",
  "scripts": {
    "clean": "rm -fR dist",
    "copy": "cp -R src dist",
    "prebuild": "npm run clean && npm run copy",
    "build": "monobrow -c monobrow.config.js",
    "start": "npm run build && ecstatic dist",
    "lint": "standard"
  },
  "author": "joshwnj",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "css-modulesify": "^0.25.1",
    "ecstatic": "^2.1.0",
    "monobrow": "^1.1.2",
    "standard": "^4.5.4"
  }
}
